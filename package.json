{
  "name": "webdriverio-api-examples",
  "version": "1.0.0",
  "description": "Provides examples of how to use the webdriver.io api. Tests executed against http://the-internet.herokuapp.com.",
  "main": "index.js",
  "scripts": {
    "test": "wdio wdio.conf.js",
    "test-headless": "wdio wdio.headless.js",
    "lint": "eslint --fix src/pages/**/*.js src/test/**/*.js src/standalone/**/*.js",
    "upgradecheck": "ncu",
    "upgradeDependencies": "ncu -u"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdavis61/webdriverio-api-examples.git"
  },
  "keywords": [
    "webdriverio",
    "selenium",
    "nodejs",
    "es6"
  ],
  "author": "James Davis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jdavis61/webdriverio-api-examples/issues"
  },
  "homepage": "https://github.com/jdavis61/webdriverio-api-examples#readme",
  "dependencies": {
    "query-string": "^6.1.0",
    "webdriverio": "^5.0.3"
  },
  "devDependencies": {
    "@wdio/cli": "^5.0.2",
    "@wdio/dot-reporter": "^5.0.0",
    "@wdio/local-runner": "^5.0.0",
    "@wdio/mocha-framework": "^5.0.0",
    "@wdio/selenium-standalone-service": "^5.0.0",
    "@wdio/spec-reporter": "^5.0.3",
    "@wdio/sync": "^5.0.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-webdriverio": "^1.0.1",
    "pre-commit": "latest"
  }
}
